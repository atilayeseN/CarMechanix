@model IPagedList<OilContRM.Models.GetRecords>
@using PagedList;
@using PagedList.Mvc;
@using OilContRM.Models;

<table>
    <thead>
        <tr>
            Licence
        </tr>
        <tr>
            Branch
        </tr>
        <tr>
            Model
        </tr>
        <tr>
            Description
        </tr>
        <tr>
            Time
        </tr>

    </thead>
    <tbody>
        @foreach(var i in Model)
        {<tr>
    <td>
        @i.LicencePlate
    </td>
    <td>
        @i.Branch
    </td>
    <td>
        @i.Model
    </td>
    <td>
        @i.Description
    </td>
    <td>
        @i.Time
    </td>
</tr>

        }
    </tbody>
</table>